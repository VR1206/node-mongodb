{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/generate-key",
      "dest": "/api/generate-key"
    },
    {
      "src": "/verify-key",
      "dest": "/api/verify-key"
    }
  ]
}
